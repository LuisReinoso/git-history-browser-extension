{
  "manifest_version": 2,
  "name": "Git History Browser Extension",
  "author": "Luis Reinoso",
  "description": "Agrega un bot√≥n a github para ver el historial del archivo / Add a button to github to see the file history",
  "homepage_url": "https://github.com/LuisReinoso/git-history-browser-extension",
  "version": "1.0.2",
  "content_scripts": [
    {
      "matches": ["https://github.com/*", "https://gitlab.com/*"],
      "js": ["main.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/git-history-16x16.png",
    "48": "icons/git-history-48x48.png",
    "128": "icons/git-history-128x128.png",
    "500": "icons/git-history-500x500.png"
  }
}
